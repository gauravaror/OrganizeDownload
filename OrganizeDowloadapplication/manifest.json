{
  "name": "Organize Download app",
  "version": "0.1",
  "manifest_version": 2,
  "description": "App to show folders and select the main",
  "permissions": [{
      "mediaGalleries": ["read", "scan", "allAutoDetected"]
  }],
  "externally_connectable": {
	"ids": ["*"],
	 "accepts_tls_channel_id": false
  },
  "app": {
     "background": {
       "scripts": ["runtime.js"]
     }
  }
}
